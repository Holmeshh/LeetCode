JavaScript
/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(nums, target) {
    var p,q;
    for(p=0;p<nums.length;p++){
        for(q=p+1;q<nums.length;q++){
            if(nums[p]+nums[q]==target)
                {
                    return [p,q];
                }
        }
    }
};
